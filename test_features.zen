set x 42
set y 10
print "x =", x
print "y =", y

set name "ZEN Language"
print "Welcome to", name

print "Testing arithmetic:"
print "x + y =", x + y
print "x - y =", x - y
print "x * y =", x * y
print "x / y =", x / y

set active true
print "Active:", active

set empty null
print "Empty:", empty

print "Testing comparisons:"
print "x > y:", x > y
print "x = y:", x = y

print "Done with tests!"