print "=== ZEN Language Feature Test ==="

print "1. Variables and Numbers:"
set a 10
set b 5
print "a =" a
print "b =" b

print "2. Arithmetic Operations:"
print "a + b =" a + b
print "a - b =" a - b
print "a * b =" a * b
print "a / b =" a / b
print "a % b =" a % b

print "3. Comparison Operations:"
print "a = 10:" a = 10
print "a != b:" a != b
print "a > b:" a > b
print "a < b:" a < b
print "a >= b:" a >= b
print "a <= b:" a <= b

print "4. Boolean Operations:"
set x true
set y false
print "x & y:" x & y
print "x | y:" x | y
print "!x:" !x
print "!y:" !y

print "5. Combined Boolean:"
set flag a > 5 & b < 10
print "Combined:" flag

print "6. Simple If Statement:"
if a > 5
    print "a is greater than 5"

print "7. Arrays:"
set numbers [1, 2, 3, 4, 5]
set fruits ["apple", "banana", "orange"]
print "Numbers:" numbers
print "Fruits:" fruits

print "8. Objects:"
set person name "Alice", age 30, active true
print "Person:" person

print "=== Test Complete ==="]