# Test special characters and edge cases
set @ "at sign"
print @

set $ "dollar"
print $

set # "hash"
print #

# Test operators as values
set op1 +
set op2 -
set op3 *
print op1

# Test expression edge cases
set x 1 / 0
print x

set y 0 / 0  
print y

set z -0
