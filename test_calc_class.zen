class Calculator
    function add a b
        return a + b
    
    function multiply a b
        return a * b
    
    function calculate operation a b
        if operation = "add"
            return self.add a b
        elif operation = "multiply"
            return self.multiply a b
        else
            return 0

set calc new Calculator
print "2 + 3 = " + calc.add 2 3
print "4 * 5 = " + calc.multiply 4 5
print "Calculate add: " + calc.calculate "add" 10 20
print "Calculate multiply: " + calc.calculate "multiply" 3 7