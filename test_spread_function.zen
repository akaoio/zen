function sum ...numbers
    set total 0
    for num in numbers
        set total total + num
    return total

function greet name ...messages
    print "Hello " + name
    for msg in messages
        print "  " + msg

set result1 sum 1 2 3 4 5
print "Sum result: " + to_string result1

greet "Alice" "Welcome!" "How are you?" "Nice to see you!"