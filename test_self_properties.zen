# Test self property access in methods
class TestClass
    set name "TestObject"
    set value 42

    function getName
        return self.name

    function getValue
        return self.value

    function setName newName
        set self.name newName

    function setValue newValue
        set self.value newValue

# Create instance
set obj new TestClass

# Test property access through methods
print "Initial name: " + obj.getName
print "Initial value: " + obj.getValue

# Test property modification through methods
obj.setName "UpdatedName"
obj.setValue 84

print "Updated name: " + obj.getName  
print "Updated value: " + obj.getValue