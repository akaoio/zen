# Panic Test Part 2 - More Extreme Cases

# Test 1: Buffer overflow attempts
set overflow ""

# Test 2: Null bytes and control characters
set nullbyte "before\0after"
set ctrl "text\x01\x02\x03\x04\x05"
set bell "\a\a\a\a\a"

# Test 3: Format string attempts
set fmt "%s%s%s%s%s%n%n%n"
set fmt2 "%x%x%x%x%x"
print fmt

# Test 4: Path traversal attempts
set path "../../../../../../../../etc/passwd"
set path2 "C:\\..\\..\\..\\..\\windows\\system32"

# Test 5: Injection attempts  
set inject "; rm -rf /"
set inject2 "'; DROP TABLE users; --"
set inject3 "`cat /etc/passwd`"
set inject4 "$(whoami)"

# Test 6: Unicode abuse
set rtl "‏Right to left text"
set zalgo "Z̴̢̺̹̈̒a̶̱̅l̷̢̘̈́g̷̱̈́̇o̸͎̊ ̶̱̌ẗ̶́ͅë̷́x̸̱̊t̵̰̂"
set emoji "😈😈😈😈😈😈😈😈😈😈😈😈😈😈😈😈😈😈😈😈"

# Test 7: Circular references
set a b
set b c  
set c a
print a

# Test 8: Type confusion
set x "123"
set y 123
set z x + y
set w x * y
set v x / y

# Test 9: Invalid UTF-8 sequences
set bad1 "\xFF\xFE"
set bad2 "\xC0\x80"
set bad3 "\xE0\x80\x80"

# Test 10: Stack smashing attempts
function bomb
    bomb
bomb

# Test 11: Integer overflows
set max 2147483647
set overflow max + 1
set underflow -2147483648 - 1

# Test 12: Float edge cases
set nan 0.0 / 0.0
set inf 1.0 / 0.0
set neginf -1.0 / 0.0

# Test 13: Parser confusion
set x 1 # set y 2
# set z 3
set a /* comment */ 4
set b 5 /* set c 6 */ 7

# Test 14: Regex bombs
set regex "(a+)+"
set regex2 "(?=a)*"

# Test 15: JSON/YAML bombs
set json '{"a":{"a":{"a":{"a":{"a":{"a":{"a":{"a":{"a":{"a":'
set yaml "a: &a [*a, *a, *a, *a, *a, *a, *a, *a, *a, *a]"