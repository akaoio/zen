# Test nested function calls
print "Max of 5 and 10: " + max 5 10
print "Max of three using nested: " + max 5 (max 10 15)

# Test string functions
set text "hello,world"
set parts split text ","
print "Split result: " + parts
print "First part uppercase: " + upper parts[0]

# Test user functions
function add a b
    return a + b

function multiply x y
    return x * y

set result add 5 (multiply 3 4)
print "5 + (3 * 4) = " + result

# Test array operations
set nums 1, 2, 3
push nums 4
print "Array after push: " + nums
set last pop nums
print "Popped value: " + last

# Test type checking
set val 42
print "Is 42 a number? " + isNumber val
print "Type of val: " + typeOf val

print "All tests completed\!"
