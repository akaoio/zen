function greet name
    return "Hello, " + name + "!"

set greeting greet "Claude"
print greeting

set numbers 1, 2, 3, 4, 5
set total 0
for n in numbers
    set total total + n

print "Total: " + total

if total > 10
    print "Total is greater than 10"
else
    print "Total is not greater than 10"

set person name "Alice", age 30
print "Name: " + person.name
print "Age: " + person.age