# Test split with variables
set text "apple,banana,orange"
set delim ","
print "Text: " + text
print "Delimiter: " + delim
set result split text delim
print "Split result: " + result

# Test user function with variables
function add a b
    return a + b

set x 5
set y 10
print "Add result: " + add x y

# Test multiple argument types
function test a b c
    print "a: " + a
    print "b: " + b  
    print "c: " + c
    return a + b + c

print "Test result: " + test 1 2 3
set result2 test x y 15
print "Test with vars: " + result2
