# Test various language features

# Arrays
set arr [1, 2, 3, 4, 5]
print "Array:" arr
print "First element:" arr[0]

# Objects  
set obj {name: "Alice", age: 30}
print "Object:" obj
print "Name:" obj.name

# Functions
function add a b
    return a + b

print "5 + 3 =" add 5 3

# Loops
print "For loop:"
for i in [1, 2, 3]
    print "  " i

print "While loop:"
set count 0
while count < 3
    print "  " count
    set count count + 1

# If statements
if 5 > 3
    print "5 is greater than 3"

# Try/catch
try
    print "In try block"
    throw "Test error"
catch e
    print "Caught:" e

