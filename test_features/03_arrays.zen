# Test 3: Arrays
print "=== Testing Arrays ==="

# Inline array
set scores 1, 2, 3, 4, 5
print "Inline array: " + scores

# Multiline array
set items
    "pen",
    "book",
    "lamp"
print "Multiline array: " + items

# Array access
print "First score: " + scores[0]
print "Second item: " + items[1]

# Array length
print "Scores length: " + scores.length
print "Items length: " + items.length

# Array methods
scores.push 6
print "After push: " + scores

set popped scores.pop()
print "Popped value: " + popped
print "After pop: " + scores

# Mixed type array
set mixed "hello", 42, true, null
print "Mixed array: " + mixed

# Nested arrays
set matrix
    1, 2, 3,
    4, 5, 6,
    7, 8, 9
print "Matrix: " + matrix