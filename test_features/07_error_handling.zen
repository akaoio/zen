# Test 7: Error Handling
print "=== Testing Error Handling ==="

# Try/catch blocks
try
    set result 10 / 0
    print "Result: " + result
catch error
    print "Caught error: " + error

# Throwing errors
function divide a b
    if b = 0
        throw "Division by zero"
    return a / b

try
    print divide 10 2
    print divide 10 0
catch e
    print "Error: " + e

# Finally block (if supported)
try
    print "In try block"
    throw "Test error"
catch e
    print "In catch block: " + e
finally
    print "In finally block"

# Multiple error types
function validateAge age
    if age < 0
        throw "Age cannot be negative"
    if age > 150
        throw "Age seems unrealistic"
    return true

try
    validateAge -5
catch error
    print "Validation error: " + error