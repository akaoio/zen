function fib n
    print "fib called with n = " + n
    if n <= 1
        return n
    set a n - 1
    set b n - 2
    print "Computing fib(" + a + ") and fib(" + b + ")"
    set fa fib a
    print "fib(" + a + ") returned " + fa
    set fb fib b
    print "fib(" + b + ") returned " + fb
    return fa + fb

print "===== Testing fib(3) ====="
set result fib 3
print "Final result: " + result