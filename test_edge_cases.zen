# Test edge cases

# Variable reference should still work
set myVar 42
set obj1 key myVar
print "Variable reference:"
print obj1

# Nested with variable reference
set nested value 100
set obj2 key nested
print "Nested variable:"
print obj2

# Multiple levels of nesting
set obj3 a b c d e "deep"
print "Deep nesting:"
print obj3

# Mixed types in nested object
set obj4 info name "John", age 30, active true, scores math 95, english 88
print "Mixed nested:"
print obj4

# Empty nested object (just keys)
set obj5 parent child grandchild
print "Keys only:"
print obj5